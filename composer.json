{
    "name": "makinacorpus/php-layout",
    "description": "Grid layout reprensentation",
    "type": "library",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:pounard/GeneratedHydrator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat-bundle.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "^3 || ^4",
        "symfony/event-dispatcher": "^3 || ^4",
        "symfony/http-kernel": "^3.1 || ^4",
        "symfony/security": "^3 || ^4"
    },
    "require-dev": {
        "makinacorpus/goat": "dev-master",
        "makinacorpus/goat-bundle": "dev-master",
        "phpunit/phpunit" : "^6"
    },
    "suggests": {
        "makinacorpus/goat": "Enables Goat storage backend",
        "makinacorpus/goat-bundle": "Uses Goat installer for managing database schema for the Goat storage backend"
    },
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Layout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakinaCorpus\\Layout\\Tests\\": "tests/"
        }
    }
}
