{
    "name": "makinacorpus/php-layout",
    "description": "Grid layout reprensentation",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "^3",
        "symfony/event-dispatcher": "^3",
        "symfony/http-kernel": "^3.1 <4",
        "symfony/security": "^3"
    },
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Layout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakinaCorpus\\Layout\\Tests\\": "tests/"
        }
    }
}
